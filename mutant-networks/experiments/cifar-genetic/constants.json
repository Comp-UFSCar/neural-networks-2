{
  "seed": 0,
  "agent_seed": 324,
  "verbose": true,
  "log_file": "./cifar-genetic-algorithm.log",
  "device": "/cpu:0",
  "n_epochs": 25,
  "n_classes": 10,
  "batch_size": 256,
  "shuffle_train": true,
  "shuffle_test": true,
  "shuffle_val": true,
  "validation_split": 0.05,
  "input_shape": [32, 32, 3],
  "architect_params": {
    "random_state": 2,
    "min_conv_layers": 2,
    "max_conv_layers": 5,
    "min_dense_layers": 0,
    "max_dense_layers": 1,
    "min_kernels": 32,
    "max_kernels": 256,
    "min_units": 32,
    "max_units": 128
  },
  "metrics_importance": {
    "validation_loss": 0.5,
    "unit_density": 0.2,
    "layer_density": 0.3
  },
  "search_params": {
    "population_size": 10,
    "max_evolution_cycles": 5,
    "mutation_factor": 0.5,
    "mutation_probability": 0.25
  }
}
